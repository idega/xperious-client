.attractions-title-section{background:#e5e5e5; border-bottom: 2px solid #559fd3;}
.attractions-title-section .site-block{padding-top: 45px; padding-bottom: 48px;}
.attractions-title-section h2{font-size:36px;color:#559fd3;font-weight:300;margin:0;line-height:38px; margin-bottom: 10px;}
.attractions-title-section h3{margin:0;font-size:18px;color:#333;line-height:18px;font-weight:300;}
.attractions-title-section .region-select{padding-left:0px;padding-top:0px;font-weight:300; padding-bottom: 12px;}
.region-select .label{text-transform:uppercase;font-size:12px;color:#333;font-weight:400; float: left; margin: 0; margin-right: 23px; padding-top: 2px;}
.region-select ul {float: left;}
.region-select li{display:inline-block;float:left;text-transform:uppercase;margin-right:20px;}
.region-select li a{display:inline-block;font-size:12px;color:#a6a49f;}
.region-select li.selected a {font-weight: 600;}


/*Attractions section -------------------------------------------------------------------------- */
.attractions-section{background:#fff;}
.attractions-section .site-block{padding-top:45px;padding-bottom:25px;}
.attractions-section .col{width:226px;float:left;margin-right:13px;}
.attractions-section .col.last{margin-right:0;}
.attractions-section .details{margin:6px 10px;margin-top:3px;margin-right:0;margin-bottom:0;padding-bottom:5px;border-bottom:1px solid #a6a49f;}
.attractions-section .element{display:block;color:#333;position:relative;margin-bottom:11px;}
.attractions-section .element:hover{background:#797979;}
.attractions-section .element .cover{left:0px;top:0px;width:226px;height:155px;position:absolute;background:url(/images/zoom-attraction.png) no-repeat center center;visibility:hidden;}
.attractions-section .element:hover .cover{visibility:visible;}
.attractions-section .element h4{font-weight:500;color:#559fd3;margin:0;font-size:16px;line-height:18px;margin-bottom:2px;}
.attractions-section .element p{font-size:12px;color:#333;line-height:17px;margin:0;}
.attractions-section .element p span{color:#559fd3;}
.attractions-section .element:hover h4{color:#fff;}
.attractions-section .element:hover p{color:#fff;}
.attractions-section .element:hover p span{color:#fff;}
.attractions-list-table{width:943px;}
.attractions-list-table td{width:226px;padding-right:13px;}
.attractions-list-table td:last-child{padding-right:0px;}

/* overrides */
.attractions-page .attractions-list .element .img{width:226px;height:155px;background-size:cover;background-position:center center;}
.attractions-page .loading{width:100%;height:180px;padding-top:120px;text-align:center;background-color:#fff}
.attractions-page .loading img{width:35px;opacity:0.75;}



/*Events*/
.events-list-page .inner-home-section{background:url(/images/events-list-bg.jpg) no-repeat fixed center top;position:relative;-webkit-background-size:auto 250px;-moz-background-size:auto 250px;-o-background-size:auto 250px;background-size:auto 250px;}
.events-list-page .inner-home-section h1{padding-top:120px;padding-bottom:73px;}
.events-list-page .inner-home-section h1 span{display:inline-block;background:url(/images/bottom-rep.png);color:#559fd3;font-weight:500;padding-bottom:6px;padding-left:12px;padding-right:12px;}

/*Attractions section title -------------------------------------------------------------------------- */
.events-title-section{background:#559fd3;}
.events-title-section .site-block{padding-top:51px;padding-bottom:38px;}
.events-title-section h2{font-size:47px;color:#333;font-weight:300;margin:0;line-height:38px;width:225px;float:left;text-transform:uppercase;line-height:25px;}
.events-title-section h2 strong{font-weight:400;color:#e5e5e5;font-size:31px;}
.events-title-section h3{margin:0;font-size:38px;color:#fff;padding-top:19px;float:left;width:500px;font-weight:300;}
.events-title-section h3 strong{font-weight:400;}

/*Events list section -------------------------------------------------------------------------- */
.events-list-section{background:#e5e5e5;}
.events-list-section .site-block{padding-top:20px;padding-bottom:81px;}

/*Months list*/
.events-list-section .month-list{padding-bottom:21px;border-bottom:1px solid #fff;margin-bottom:32px;width:945px;overflow:hidden;}
.month-list li{float:left;padding-right:31px;margin-right:8px;background:url(/images/month-dots.png) no-repeat right center;}
.month-list li:last-child{padding-right:0;margin-right:0;background:none;}
.month-list li a{display:inline-block;padding:0 8px;line-height:22px;color:#333;font-weight:500;font-size:12px;text-transform:uppercase;}
.month-list li a:hover,
.month-list li a.active{border-radius:3px;background:#559fd3;color:#fff;}

/*Events listing*/
.events-listing .element{width:189px;height:185px;float:left;position:relative;background:#86c543;color:#fff;}
.events-listing .element .in{padding:9px 14px;}
.events-listing .date{color:#fff;font-size:28px;font-weight:600;margin:0;margin-bottom:10px;}
.events-listing .text{margin:0;font-size:13px;}
.events-listing .text strong{display:block;font-weight:600;}
.events-listing .element.double{width:378px;}
.events-listing .element.empty,.events-listing .element.empty:hover{background:none;}
.month-title{font-size:51px;color:#559fd3;font-weight:300;line-height:52px;padding:6px 0 10px 0;}
.month-title strong{font-weight:400;}

/*Nips*/
.events-listing .element .nip{display:inline-block;position:absolute;z-index:10;}
.events-listing .nip-left .nip,
.events-listing .nip-right .nip{width:11px;height:22px;}
.events-listing .nip-top .nip,
.events-listing .nip-bottom .nip{width:22px;height:11px;}
.events-listing .nip-top .nip{background:url(/images/arrows-top.png) no-repeat left top;top:-11px;left:20px;}
.events-listing .nip-left .nip{background:url(/images/arrows-left.png) no-repeat left top;left:-11px;top:20px;}
.events-listing .nip-bottom .nip{background:url(/images/arrows-down.png) no-repeat left top;bottom:-11px;right:20px;}
.events-listing .nip-right .nip{background:url(/images/arrows-right.png) no-repeat left top;top:20px;right:-11px;}

