/*Font declarations*/
body, button,input,select,textarea, .ui-widget{font-family: 'Ubuntu', sans-serif; font-weight: 300;}

/*---Default elements---*/
a{margin:0;padding:0;font-size:100%;vertical-align:baseline;text-decoration:none;color:#86c543;}
a:hover,a:focus{outline:none;}
a img{border:none;}
img{vertical-align:middle;-ms-interpolation-mode:bicubic;}
b,strong{font-weight:500;}
p{line-height:1.2em;margin:12px 0;}
hr{display:block;height:1px;border:0;border-top:1px solid #e5e5e5;margin:1em 0;padding:0;}
h1,h2,h3,h4,h5,h6{margin:12px 0;font-weight:300;}

/*Tables*/
table{border-collapse:collapse;border-spacing:0;}
table.full-table{width:100%;}
td,td img{vertical-align:top;}

/*Forms*/
form{margin:0;}
textarea{overflow:auto;vertical-align:top;}
fieldset{border:0;margin:0;padding:0;}
legend{border:0;margin-left:-7px;padding:0;}
input[type="checkbox"],input[type="radio"]{box-sizing:border-box;}
input[type="search"]{-moz-box-sizing:content-box;-webkit-box-sizing:content-box;box-sizing:content-box;}
button,input[type="button"],input[type="reset"],input[type="submit"]{cursor:pointer;-webkit-appearance:button;}
button,input{line-height:normal;overflow:visible;}
.form-row input[type="text"],.form-row label{display:inline-block;float:left;}
label.for-input{width:auto !important;cursor:pointer}
input.radio,input.checkbox{vertical-align:text-bottom;margin:0;}
input[type="text"],input[type="password"],textarea,select{border:1px solid #ccc;position:relative;}
button::-moz-focus-inner,input::-moz-focus-inner{border:0;padding:0;}
button{width:auto;overflow:visible;}
button,input,select,textarea{font-size:100%;margin:0;vertical-align:baseline;vertical-align:middle;outline:none;outline:none;}

/*Placeholders*/
.placeholder{color:#6d6d6d;font-weight:300;}
::-webkit-input-placeholder{color:#6d6d6d;font-weight:300;}
:-moz-placeholder{color:#6d6d6d;font-weight:300;}
::-moz-placeholder{color:#6d6d6d;font-weight:300;opacity:1;}
:-ms-input-placeholder{color:#6d6d6d;font-weight:300;}

/*Custom buttons*/
.button{padding-top:0;padding-bottom:0;display:inline-block;border-radius:4px;background:#86c543; padding-left:16px; padding-right: 16px; height:40px;line-height:40px;text-transform:uppercase;color:#fff;font-size:14px;border:none;text-align:left;position:relative;
    transition: background-color 0.2s linear;
}
.button:hover {background: #53762b; transition: background-color 0.2s linear;}

.button-grey {background: #a6a49f;}
.button-grey:hover {background: #605f5c;}


/*Autocomplete*/
.ui-autocomplete{padding:0;background:#fff;}
.ui-menu .ui-menu-item{border:none;background:none !important;margin:0 !important;}
.ui-menu .ui-menu-item a{padding:4px 17px;font-size:14px;color:#6d6d6d;border:none;background:none !important;margin:0 !important;}
.ui-menu .ui-menu-item a:hover{color:#fff;background:#86c543 !important;}
.ui-menu .ui-menu-item a.ui-state-focus{color:#fff;background:#86c543 !important;}

/*Custom Selectmenu*/
.ui-selectmenu{display:block;display:inline-block;position:relative;height:44px;vertical-align:middle;text-decoration:none;overflow:hidden;zoom:1;float:left;border:none;background:#fff;border-radius:4px;font-weight:300;}
.ui-selectmenu.ui-corner-top{border-radius:4px 4px 0 0;}
.ui-selectmenu.to-top{border-radius:0 0 4px 4px;}
.ui-selectmenu-icon{position:absolute;right:6px;margin-top:-8px;top:50%;}
.ui-selectmenu-menu{padding:0;margin:0;position:absolute;top:0;display:none;z-index:1005;}

/* z-index:1005 to make selectmenu work with dialog */
.ui-selectmenu-menu ul{padding:0;margin:0;list-style:none;position:relative;overflow:auto;overflow-y:auto ;overflow-x:hidden;-webkit-overflow-scrolling:touch;}
.ui-selectmenu-open{display:block;}
.ui-selectmenu-menu-popup{margin-top:-1px;}
.ui-selectmenu-menu li{padding:0;margin:0;display:block;border-top:1px dotted transparent;border-bottom:1px dotted transparent;border-right-width:0 !important;border-left-width:0 !important;font-weight:normal !important;}
.ui-selectmenu-menu li a,.ui-selectmenu-status{line-height:1.4em;display:block;padding:.405em 2.1em .405em 1em;outline:none;text-decoration:none;}
.ui-selectmenu-menu li.ui-state-disabled a,.ui-state-disabled{cursor:default;}
.ui-selectmenu-menu li.ui-selectmenu-hasIcon a,
.ui-selectmenu-hasIcon .ui-selectmenu-status{padding-left:20px;position:relative;margin-left:5px;}
.ui-selectmenu-menu li .ui-icon,.ui-selectmenu-status .ui-icon{position:absolute;top:1em;margin-top:-8px;left:0;}
.ui-selectmenu-status{line-height:1.4em;}
.ui-selectmenu-menu li span,.ui-selectmenu-status span{display:block;margin-bottom:.2em;}
.ui-selectmenu-menu li .ui-selectmenu-item-header{font-weight:bold;}
.ui-selectmenu-menu li .ui-selectmenu-item-footer{opacity:.8;}
.ui-selectmenu-icon,
.ui-state-default .ui-icon{width:38px;height:38px;background:url(/images/select-down.png) no-repeat left top;margin:0;right:1px;top:3px;}
.ui-selectmenu-menu li a,.ui-selectmenu-status{font-size:16px;color:#6D6D6D;font-weight:300;padding-top:0;padding-bottom:0;line-height:44px;}
.ui-selectmenu-menu li a,.ui-selectmenu-status{padding-left:13px;padding-right:38px;}
.ui-selectmenu-menu ul{background:#fff;border:none;box-shadow:0 3px 3px #eee;}
.ui-selectmenu-menu ul.to-top{box-shadow:none;border-radius:4px 4px 0 0;}
.convert-form .ui-selectmenu-menu ul{box-shadow:0 3px 3px #555;}
.ui-selectmenu-menu li,
.ui-selectmenu-menu li a,
.ui-selectmenu-menu li.ui-item-selected,
.ui-selectmenu-menu li.ui-state-hover{border:none;background:none;}
.ui-selectmenu-menu li a{border-top:1px solid #E3E2E1;}
.ui-selectmenu-menu .to-top li a{border-top:none;border-bottom:1px solid #E3E2E1;}
.ui-selectmenu-menu li a:hover{color:#999;}

/*
 * JQuery UI theme tweaks for one month calendar.
 */
.calendar .ui-widget-header{background:rgba(255,255,255,0);border:0px;}
.calendar.ui-widget-content{border:0px;background:rgba(255,255,255,0);padding:0px;}
.calendar .ui-dialog-titlebar{display:none;}
.calendar .ui-dialog-content{margin-top:3px;padding:7px 0 0 0;background-image:url('/images/calendar-nip.png');background-repeat:no-repeat;background-position:130px 0px;}
.calendar .titlebar{border-radius:3px 3px 0px 0px;background-color:#e5e5e5;height:35px;padding-left:5px;}
.calendar .titlebar .arrival-departure{font-size:16px;font-family:'Ubuntu',sans-serif;line-height:35px;margin-left:5px;}
.calendar .titlebar .selection{margin-left:5px;color:#333333;font-size:16px;font-weight:300;font-family:'Ubuntu',sans-serif;}
.calendar .titlebar sup{font-size:10px}
.calendar .titlebar .selection strong{font-weight:500}
.calendar .close{padding-right:25px;padding-top:10px;font-size:14px;height:18px;float:right;width:50px;background-image:url('/images/timeframe-next.png');background-repeat:no-repeat;background-position:35px 8px;}
.calendar .datepicker{padding-top:0px;padding-left:0px;padding-right:6px;background-image:url('/images/timeframe-bg.png');}
.calendar .ui-datepicker{width:100% !important;padding:.2em .2em 0;display:none;background:rgba(255,255,255,0);border:0px;}
.calendar .ui-datepicker .ui-state-default{border:0px;font-size:16px;font-weight:300;background:rgba(255,255,255,0);color:#333333;}
.calendar .ui-datepicker .ui-state-hover{border:0px;top:0px;right:0px;background-color:#86c543;}
.calendar .ui-datepicker .ui-state-highlight{border:0px;background:rgba(255,255,255,0);}
.calendar .ui-datepicker .ui-state-highlight .ui-state-default{border:0px;background:#86c543;}
.calendar .ui-datepicker .ui-state-active{border:0px}
.calendar .ui-datepicker-today .ui-state-default{border:0px;background:rgba(255,255,255,0);}
.calendar .ui-datepicker-today .ui-state-default.ui-state-hover{border:0px;background-color:#86c543;}
.calendar .ui-datepicker .ui-datepicker-header{position:relative;padding:.2em 0;background:rgba(255,255,255,0);border:0px;}
.calendar .ui-datepicker .ui-datepicker-prev,
.calendar .ui-datepicker .ui-datepicker-next{position:absolute;top:8px;width:1.8em;height:1.8em;}
.calendar .ui-datepicker .ui-datepicker-next{left:220px;right:0px;}
.calendar .ui-icon{width:40px;height:15px;text-transform:uppercase;font-size:10px;font-weight:300;color:#333333;padding-top:2px;}
.calendar .ui-icon-circle-triangle-e{background-image:url('/images/next-2.png') !important;background-position:27px 0px;background-size:6px 13px;text-indent:0px;}
.calendar .ui-icon-circle-triangle-w{background-image:url('/images/prev-2.png') !important;background-position:0px 0px;background-size:6px 13px;text-indent:12px;}
.calendar .ui-datepicker .ui-datepicker-prev-hover,
.calendar .ui-datepicker .ui-datepicker-next-hover{background:rgba(255,255,255,0)}
.calendar .ui-datepicker .ui-datepicker-prev-hover{left:2px}
.calendar .ui-datepicker .ui-datepicker-next-hover{right:2px}
.calendar .ui-datepicker .ui-datepicker-prev span,
.calendar .ui-datepicker .ui-datepicker-next span{display:block;position:absolute;left:50%;margin-left:-8px;top:50%;margin-top:-8px;cursor:hand;cursor:pointer;}
.calendar .ui-datepicker .ui-datepicker-title{margin:0em;line-height:1.8em;text-align:center;font-size:1.4em;font-weight:300;color:#86c543;}
.calendar .ui-datepicker .ui-datepicker-title select{font-size:1em;margin:1px 0;}
.calendar .ui-datepicker select.ui-datepicker-month-year{width:100%}
.calendar .ui-datepicker select.ui-datepicker-month,
.calendar .ui-datepicker select.ui-datepicker-year{width:49%}
.calendar .ui-datepicker-month{font-family:'Ubuntu',sans-serif;font-weight:400;font-size:16px;}
.calendar .ui-datepicker-year{font-family:'Ubuntu',sans-serif;font-weight:400;font-size:16px;}
.calendar .ui-datepicker-group-first .ui-datepicker-title{text-align:right;width:100%;}
.calendar .ui-datepicker-group-first .ui-datepicker-year{margin-right:10px}
.calendar .ui-datepicker-group-last .ui-datepicker-month{margin-left:25px}
.calendar .ui-datepicker-group-last .ui-datepicker-title{text-align:left;width:100%;}
.calendar .ui-datepicker table{width:240px;font-size:.9em;border-collapse:collapse;margin:0px 0px 10px 10px;/* padding-left:25px;*/ /* padding-right:10px;*/}
.calendar .ui-datepicker th{/* padding:.7em .3em;*/ text-align:center;font-weight:300;text-align:right;border:0;font-size:10px;text-transform:uppercase;font-family:'Ubuntu',sans-serif;}
.calendar .ui-datepicker-week-end{color:#9b1110}
.calendar .ui-datepicker td{border:0;/* padding:3px;*/}
.calendar .ui-datepicker td span,
.calendar .ui-datepicker td a{display:block;/* padding:.2em;*/ text-align:right;text-decoration:none;font-family:'Ubuntu',sans-serif;}
.calendar .ui-datepicker .ui-datepicker-buttonpane{background-image:none;margin:.7em 0 0 0;padding:0 .2em;border-left:0;border-right:0;border-bottom:0;}
.calendar .ui-datepicker .ui-datepicker-buttonpane button{float:right;margin:.5em .2em .4em;cursor:pointer;padding:.2em .6em .3em .6em;width:auto;overflow:visible;}
.calendar .ui-datepicker .ui-datepicker-buttonpane button.ui-datepicker-current{float:left}

/* with multiple calendars */
.calendar .ui-datepicker.ui-datepicker-multi{width:auto}
.calendar .ui-datepicker-multi .ui-datepicker-group{float:left}
.calendar .ui-datepicker-multi .ui-datepicker-group table{width:95%;margin:0 auto .4em;}
.calendar .ui-datepicker-multi-2 .ui-datepicker-group{width:370px}
.calendar .ui-datepicker-group-first{padding-right:30px}
.calendar .ui-datepicker-multi-3 .ui-datepicker-group{width:33.3%}
.calendar .ui-datepicker-multi-4 .ui-datepicker-group{width:25%}
.calendar .ui-datepicker-multi .ui-datepicker-group-last .ui-datepicker-header,
.calendar .ui-datepicker-multi .ui-datepicker-group-middle .ui-datepicker-header{border-left-width:0}
.calendar .ui-datepicker-multi .ui-datepicker-buttonpane{clear:left}
.calendar .ui-datepicker-row-break{clear:both;width:100%;font-size:0;}

/* RTL support */
.calendar .ui-datepicker-rtl{direction:rtl}
.calendar .ui-datepicker-rtl .ui-datepicker-prev{right:2px;left:auto;}
.calendar .ui-datepicker-rtl .ui-datepicker-next{left:2px;right:auto;}
.calendar .ui-datepicker-rtl .ui-datepicker-prev:hover{right:1px;left:auto;}
.calendar .ui-datepicker-rtl .ui-datepicker-next:hover{left:1px;right:auto;}
.calendar .ui-datepicker-rtl .ui-datepicker-buttonpane{clear:right}
.calendar .ui-datepicker-rtl .ui-datepicker-buttonpane button{float:left}
.calendar .ui-datepicker-rtl .ui-datepicker-buttonpane button.ui-datepicker-current,
.calendar .ui-datepicker-rtl .ui-datepicker-group{float:right}
.calendar .ui-datepicker-rtl .ui-datepicker-group-last .ui-datepicker-header,
.calendar .ui-datepicker-rtl .ui-datepicker-group-middle .ui-datepicker-header{border-right-width:0;border-left-width:1px;}

