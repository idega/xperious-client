/*Favorite theme section ----------------------------*/
.favorite-theme-section{background:url(/images/themes-section-bg.jpg) no-repeat center center fixed;padding-top:30px;padding-bottom:39px;-webkit-background-size:cover;-moz-background-size:cover;-o-background-size:cover;background-size:cover;}
