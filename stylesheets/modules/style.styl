.top-menu a{display:inline-block;font-size:14px;color:#fff;line-height:48px;}
.top-menu a:hover{color:#86c543;}
.top-menu > li.has-submenu > a:hover,.top-menu > li.has-submenu:hover > a{background:url(/images/top-menu-active.png) no-repeat center bottom;}
.small-main-logo{overflow:hidden;text-align:left;-moz-transition:width .5s;-ms-transition:width .5s;-o-transition:width .5s;-webkit-transition:width .5s;transition:width .5s;width:0;}
