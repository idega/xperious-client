.top-menu__submenu a{line-height:26px;}
.top-menu__submenu .menu{padding-left:35px;}
.top-menu__submenu li{margin-right:27px;}
li:hover .top-menu__submenu{-moz-transition:max-height .5s .5s;-ms-transition:max-height .5s .5s;-o-transition:max-height .5s .5s;-webkit-transition:max-height .5s .5s;transition:max-height .5s .5s;max-height:340px;}
.submenu-cols-5 .menu{width:175px;float:left;margin-right:15px;padding-left:0px;}
.submenu-cols-6 .menu{width:120px;float:left;margin-right:15px;padding-left:0px;}
.submenu-cols-5 .menu li,
.submenu-cols-6 .menu li{display:block;padding:0;margin:0;float:none;text-align:left}
.site-header__selected-location{line-height:40px;font-size:14px;padding-left:15px;}

/*Sticky top menu*/
.sticky-top-menu-section{background:#5c5c5c;border-top:1px solid #c4daac;position:relative;z-index:30;}
