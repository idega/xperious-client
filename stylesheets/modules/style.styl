.ico-calendar{background-image:url(/images/calendar-ico.png);background-repeat:no-repeat;background-position:9px 10px;}
.ico-guest{background-image:url(/images/guest-ico.png);background-repeat:no-repeat;background-position:9px 10px;}
.ico-event{background-image:url(/iages/event-ico.png);background-repeat:no-repeat;background-position:9px 10px;}
.planner input[type="text"]{padding:10px 16px;width:414px;font-size:14px;border-radius:4px 0 0 4px;}
.planner .button{line-height:40px;height:40px;background:#86c543 url(/images/search-ico-white.png) no-repeat center center;width:45px;text-align:left;padding-bottom:1px;font-size:14px;border-radius:0 4px 4px 0;overflow:hidden;text-indent:-999px;}
.no-js .planner .button:hover,.planner .button.hovered{background-color:#53762b;}
.planer-slide-down{display:inline-block;position:absolute;width:32px;height:31px;cursor:pointer;background:url(/images/icon-down.png) no-repeat left top;left:50%;margin-left:-16px;bottom:50px;}
