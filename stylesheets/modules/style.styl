.closed .destination-section__map{height:0;}

/*Langiage menu*/
.lang-menu{float:right;padding-right:6px;padding-top:6px;}
.lang-menu li{float:left;margin-right:4px;}
.lang-menu a{display:inline-block;width:18px;height:14px;}
.lang-menu .en{background:url(/images/lang-uk.png) no-repeat left top;}
.lang-menu .ice{background:url(/images/lang-ice.png) no-repeat left top;}
.lang-menu .active,
