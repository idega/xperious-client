.sign-in-section h2 span{text-transform:none;color:#a6a49f;font-size:14px;}
.signin-info{font-size:14px;color:#a6a49f;margin:0;padding-top:9px;width:500px;float:left;text-align:left;}
.signin-links{width:250px;float:right;font-size:14px;padding-top:7px;padding-bottom:7px;}
.fb-login{display:inline-block;float:left;color:#fff;padding-left:37px;line-height:34px;padding-right:8px;border-radius:4px;background:#4466a9 url(/images/fb-button.png) no-repeat left top;font-size:14px;margin-right:5px;}
