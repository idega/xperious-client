.sign-in-section input[type="text"],
.sign-in-section input[type="password"]{font-size:14px;padding:7px 12px;width:220px;margin-right:5px;}
.sign-in-section .button{background:#86c543;width:110px;float:left;height:34px;font-size:14px;text-transform:none;text-align:center;padding:0 10px;line-height:34px;}

/*Header desination selection*/
.header-destination-section.closed{overflow:hidden;}
.header-destination-section{position:relative;}
.destination-section__title{background:#86c543;font-size:36px;color:#333;overflow:hidden;}
.destination-section__title-in{padding-top:20px;padding-bottom:20px;}
.destination-section__close{display:inline-block;position:absolute;cursor:pointer;width:32px;height:32px;background:url(/images/destination-close.png) no-repeat center center;top:65px;right:65px;z-index:10;}
