.sticky-top-menu-section .top-menu > li > a{line-height:48px;}
.sticky-top-menu-section .top-menu__submenu{top:49px;}
.sticky-top-menu-section .chose-destination{line-height:48px;}

/*Signin section*/
.header-signin-form{width:774px;}
.sign-in-section{background:#333;border-top:1px solid #7e9566;margin:0;overflow:hidden;height:0px;}
.sign-in-section .site-block-in{padding:9px 0;}
