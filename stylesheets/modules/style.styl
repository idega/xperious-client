/*Travel options section*/
.travel-options-section{background:#fff;text-align:center;color:#333;font-size:20px; line-height: 27px; height:0;overflow:hidden;-moz-transition:height .5s;-ms-transition:height .5s;-o-transition:height .5s;-webkit-transition:height .5s;transition:height .5s;}
